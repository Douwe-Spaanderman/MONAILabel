{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "simple": {
      "format": "[%(asctime)s] [%(levelname)s] (%(name)s) - %(message)s"
    },
    "debug": {
      "format": "[%(asctime)s] [%(levelname)s] (%(name)s.%(funcName)s:%(lineno)d) - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "simple"
    },
    "file_handler": {
      "class": "logging.handlers.RotatingFileHandler",
      "formatter": "simple",
      "filename": "/workspace/dlmed/MONAI-label/server/workspace/apps/segmentation_spleen/logs/app.log",
      "backupCount": 10,
      "maxBytes": 10485760,
      "encoding": "utf-8"
    }
  },
  "root": {
    "level": "INFO",
    "propagate": false,
    "handlers": [
      "console"
    ]
  },
  "loggers": {
    "server": {
      "level": "INFO",
      "propagate": false,
      "handlers": [
        "file_handler"
      ]
    }
  }
}